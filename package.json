{
  "name": "file-mover",
  "version": "0.0.5",
  "description": "small library that will move files from one folder to another",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist/",
    "dev": "NODE_ENV=development npm run build && node --trace-warnings dist/src/index.js",
    "start": "NODE_ENV=production node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oforfamar/file-mover.git"
  },
  "keywords": [
    "nodejs",
    "streams",
    "file system"
  ],
  "author": "Tudor Roman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oforfamar/file-mover/issues"
  },
  "homepage": "https://github.com/oforfamar/file-mover#readme",
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "rimraf": "^4.1.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "mongoose": "^6.9.2"
  }
}
